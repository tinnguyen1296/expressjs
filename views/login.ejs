<h1>Login</h1>
<form method="post">
  <% if (errors) { %>
        <div class="alert alert-danger" role="alert">
          <%= errors %>
        </div>
  <% } %>
  <div class="form-group">
    <label for="exampleInputEmail1">Email address</label>
    <input type="email" name="email" class="form-control" value="<%= values.email %>" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input type="password" name="password" value="<%= values.password %>" class="form-control" id="exampleInputPassword1" placeholder="Password">
  </div>
  <input type="submit" class="btn btn-primary" name="" value="Login">
</form>
